<div class="row">
  <div class="container">
    <div class="col-md-8">
      
      <h1><%= @user.name %>'s Contributions</h1>
        <h4>Original author of <%= pluralize(@wikis.count, 'wiki') %></h4>
          <ol>
            <% @wikis.each do |wiki| %>
              <li><%= link_to wiki.title, wiki %></li>
            <% end %>
          </ol>

        <h4>Collaborator on <%= pluralize(@user.wikis.count, 'wiki') %></h4>
          <ol>
            <% @user.wikis.each do |wiki| %>
                <li><%= link_to wiki.title, wiki %></li>
            <% end %>
          </ol>

    </div>
  </div>
</div>
